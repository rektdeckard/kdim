{
  "name": "kdim",
  "version": "0.6.2",
  "description": "Utility data stuctures, math, and types for JS",
  "author": {
    "name": "Tobias Fried",
    "email": "friedtm@gmail.com",
    "url": "https://tobiasfried.com"
  },
  "repository": "rektdeckard/kdim",
  "keywords": [
    "js",
    "ts",
    "node-js",
    "data-structures",
    "math",
    "utilities"
  ],
  "license": "MIT",
  "private": false,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write ./src/** ./test/** *.json",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "ex": "vite serve ./example",
    "ex:serve": "yarn run ex",
    "ex:build": "vite build ./example",
    "ex:deploy": "yarn run ex:build && gh-pages -d ./example/dist"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@vitest/coverage-v8": "^0.34.1",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  }
}
